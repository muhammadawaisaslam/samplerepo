{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "demodfforclient"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CoreHR_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CoreHR'"
		},
		"AdventureWorks_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AdventureWorks_ls'"
		},
		"ADLGen2_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLGen2_ls'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://demokvforclient.vault.azure.net/"
		},
		"ADLGen2_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://monsteradl.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "demodfforclient_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"CoreHR_connectionString": {
						"value": "[parameters('CoreHR_connectionString')]"
					},
					"AdventureWorks_ls_connectionString": {
						"value": "[parameters('AdventureWorks_ls_connectionString')]"
					},
					"ADLGen2_ls_accountKey": {
						"value": "[parameters('ADLGen2_ls_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"ADLGen2_ls_properties_typeProperties_url": {
						"value": "[parameters('ADLGen2_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}